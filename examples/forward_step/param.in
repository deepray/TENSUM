grid
{
   cell   median
}

numeric
{
   time_mode    unsteady
   time_scheme  ssprk3
   time_step    0.0
   cfl          0.8
   max_iter     50000
   final_time   4.0
   min_residue  1.0e-20
   reconstruct  
   {
      method    minmod
   }
   bc_scheme    weak
   liou_fix     yes
   sample_list
   {
	  groups
	  { 
	  }
	  free_list
	  {
             2
	  }   
   }
   rnd_per_sample  0
   rnd_file_loc    rnd_nos.dat
   rnd_file_hook   0
}

material
{
   gamma       1.4
   gas_const   7.14285714285714e-01 // R = 1/gamma
   viscosity   
   {
      model    constant
      mu_ref   0.0
   }
   prandtl     1.0
   model       euler
   flux        kepes_tecno_roe
   flux_par
   {
      alpha      0
      beta       0
   }
   flux_correction   no
   balance
   {
      switch_active  no
      ND             yes
      HEAT           yes
      VISC           yes
   }
}

constants
{
   T    1.0
   p    1.0
   u    3.0
}

initial_condition
{
   temperature  T
   xvelocity    u
   yvelocity    0.0
   zvelocity    0.0
   pressure     p
}

boundary
{
   100001
   {
      type         farfield
      temperature  T
      xvelocity    u
      yvelocity    0.0
      zvelocity    0.0
      pressure     p
   }

   100002
   {
      type         slip
   }

   100003
   {
      type         outlet
   }

}

exact_soln
{
   available    no
}

integrals
{
}

output
{
   format                vtk
   frequency             100
   time_stamps           1
   use_online_stat_tool  no
   variables
   {
      density
      mach
   }
   surfaces
   {
   }
   pdf_points
   {
   }
   restart               yes
   restart_frequency     100
   log_output            yes
   soln_output           yes
   save_global           yes
   save_mesh_Pe          no
   find_error            no
}
